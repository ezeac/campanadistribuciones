button.menu-toggle {
    position: absolute;
    right: 5%;
    top: 19px;
    border: none;
    height: 40px;
    width: 40px;
    padding: 0;
}

button.menu-toggle:after {
    display: none;
}

button.menu-toggle:before {
    content: 'menu';
    font-family: 'Material Icons';
    white-space: nowrap;
    font-size: 32px;
    background: none;
    text-align: center;
    position: absolute;
    top: 50%;
    left: 50%;
    width: auto;
    height: auto;
    transform: translate(-50%,-50%) !important;
    font-weight: 300;
    color: black;
}

.toggled button.menu-toggle:before {
    content: 'close';
}

button.menu-toggle > span {
    display: none;
}

#site-navigation > .menu {
    display: none;
}

.primary-navigation {
    border: 0;
}

.woocommerce-active .site-header .site-branding {
    width: 80%;
    margin-top: 10px;
}

.top-slider .carousel-caption {
    width: 90%;
    margin: 0 5%;
}

.counter {
    display: none;
}

.owl-nav {
    width: 130%;
